package collection;

import java.util.HashMap;

public class C011_HashMapDemo {
	public static void main(String[] args) {
		HashMap<String,Integer> map = new HashMap<>();
		map.put("Java", 50);
		map.put("php", 51);
		map.put("Android", 49);
		map.put("Python", 45);
		map.put("Java", 52);
		
//		System.out.println(map);
		
		System.out.println(map.get("Java"));
	}

}
