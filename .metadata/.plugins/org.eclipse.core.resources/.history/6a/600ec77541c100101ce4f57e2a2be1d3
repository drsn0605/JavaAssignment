package CollectionInterface;

public class Player implements Comparable<Player>{
	int id;
	String name;
	String type;
	int age;
	public Player(int id, String name, String type, int age) {
		super();
		this.id = id;
		this.name = name;
		this.type = type;
		this.age = age;
	}
	@Override
	public String toString() {
		return "Player [id=" + id + ", name=" + name + ", type=" + type + ", age=" + age + "]";
	}
	
	//10 25 9 4
	@Override
	public int compareTo(Player o) {
		int currentId = o.id;
		int newId = this.id;
		
		int i = 0;
		if(currentId>newId) {
			i = -1;
		} 
		else if (currentId<newId) {
			i = 1;
		}
		else {
			i = 0;
		}
		
		return i;
	}
	
	

}
